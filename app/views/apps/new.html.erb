<div class="row">
  <div class="col s12 m4">
    <ul class="collection with-header">
      <li class="collection-header">
        <h4>Apps</h4>
        <%= link_to 'New App', new_app_path %>
      </li>
      <% @apps.each do |app| %>
        <%= link_to app.name, app, class: 'collection-item'  %>
      <% end %>
    </ul>
  </div>
  <div class="col s12 m8">
    <h1>New App</h1>
    <%= render 'form' %>
  </div>
</div>
