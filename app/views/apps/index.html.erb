<div class="row">
  <div class="col s12 m4">
    <ul class="collection with-header apps">
      <li class="collection-header">
        <h4>Apps</h4>
        <%= link_to 'New App', new_app_path, remote: true %>
      </li>
      <% @apps.each do |app| %>
        <%= link_to app.name, app, class: 'collection-item', remote: true  %>
      <% end %>
    </ul>
  </div>
  <div class="col s12 m8">
    <div class="inner"></div>
  </div>
</div>