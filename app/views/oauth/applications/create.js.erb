<% unless @application.errors.empty? %>
  <% @application.errors.full_messages.each do |message| %>
    US.Utility.toastError('<%= message %>')
  <% end %>
<% else %>
  US.Utility.toastSuccess('The app has been created successfully')
  $('.new_app').replaceWith('<%= j render partial: 'oauth/applications/application' %>')
<% end %>